escape C-y
unbind A
unbind C-A
unbind 0
unbind 1
unbind 2
unbind 3
unbind 4
unbind 5
unbind 6
unbind 7
unbind 8
unbind 9
unbind minus
unbind i
unbind C-i
unbind v
unbind C-v
unbind V
unbind C-V
unbind f
unbind C-f
unbind F
unbind apostrophe
unbind C-apostrophe
unbind x
unbind C-x
unbind b
unbind C-b

bind w exec google-chrome
definekey top s-w exec google-chrome
definekey top s-c exec x-terminal-emulator

unbind exclam
unbind C-exclam
unbind Return
unbind C-Return
unbind p
unbind C-p
unbind space
unbind C-space
bind h prev
bind C-h prev

bind C-slash help root
bind question help root

bind slash help top

definekey top s-Left focusleft
definekey top s-Right focusright
definekey top s-Down focusdown
definekey top s-Up focusup

definekey top s-Tab focus
definekey top s-ISO_Left_Tab focuslast

definekey top s-backslash hsplit
definekey top s-bar hsplit
definekey top s-minus split
definekey top s-underscore split

set resizeunit 120
set border 0
set gravity center
set bargravity center

set font xft:sans-serif:pixelsize=64
set fgcolor yellow
set bgcolor darkblue
set inputwidth 1024
